{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["cython"],
      "matchCurrentValue": "<=3.1.5",
      "allowedVersions": "<=3.1.5"
    }
  ],
  "lockFileMaintenance": {
    "enabled": true
  }
}
